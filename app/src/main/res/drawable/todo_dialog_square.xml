<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/square"
    android:shape="rectangle"
    android:padding="10dp">

    <stroke
        android:width="0dp"
        android:color="#00000000" />
    <solid
        android:id="@+id/color"
        android:color="#FFD5D5" />
    <size
        android:width="30dp"
        android:height="30dp" />
    <corners
        android:radius="6dp" />
</shape>